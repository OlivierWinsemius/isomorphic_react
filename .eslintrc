{
    "extends": ["prettier", "airbnb"],
    "parser": "babel-eslint",
    "rules": {
        "indent": "off",
        "comma-dangle": ["error", "always-multiline"],
        "react/jsx-filename-extension": [1, { "extensions": [".js"] }]
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "settings": {
        "import/resolver": {
            "webpack": { "config": "./webpack.common.js" }
        }
    }
}
